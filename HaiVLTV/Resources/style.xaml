<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone"
   
    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone">
    <!-- TextBlock styles -->
    <Style x:Key="TextBlockLargeStyle" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="/Fonts/boombox2.ttf#BoomBox 2" />
        <Setter Property="FontSize" Value="40" />
    </Style>
    <Style x:Key="TextBlockAboutLargeStyle" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="/Fonts/JINGJING.TTF#Jing Jing" />
        <Setter Property="FontSize" Value="35" />
    </Style>
    <Style x:Key="TextBlockDem1" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="/Fonts/DS-DIGI.TTF#DS-Digital" />
        <Setter Property="FontSize" Value="40" />
        <Setter Property="Foreground" Value="#ff1c6e"/>
    </Style>
    <Style x:Key="TextBlockDem" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="/Assets/Fonts/DS-DIGI.TTF#DS-Digital" />
        <Setter Property="FontSize" Value="30" />
    </Style>
    <Style x:Key="stylenoidung1" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="/Fonts/TREBUC.TTF#Trebuchet MS" />
        <Setter Property="FontSize" Value="18"/>
    </Style>

</ResourceDictionary>